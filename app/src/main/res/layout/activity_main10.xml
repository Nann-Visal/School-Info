<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blues_color"
    tools:context=".MainActivity10">
    <android.support.design.widget.BottomNavigationView
        android:id="@+id/back_navigation_bar_ac10"
        android:layout_width="419dp"
        android:layout_height="64dp"
        android:background="#4FB5B226"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001"
        app:menu="@menu/button_back_of_navigation_bar" />
    <android.support.design.widget.BottomNavigationView
        android:id="@+id/button_navigation_bar_ac10"
        android:layout_width="423dp"
        android:layout_height="51dp"
        android:background="#939F83"
        android:scaleX="1"
        android:scaleY="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.998"
        app:menu="@menu/my_button_of_navigation_bar" />
    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="133dp"
        android:layout_height="133dp"
        android:layout_marginBottom="544dp"
        app:layout_constraintBottom_toTopOf="@+id/button_navigation_bar_ac10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/school_icon" />

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="47dp"
        android:background="@drawable/my_background_button_in_setting"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/button_navigation_bar_ac10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_navigation_bar_ac10"
        app:layout_constraintVertical_bias="0.166">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Contact Us"
            android:textColor="#0E0E0E"
            android:textSize="34sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="381dp"
        android:layout_height="410dp"
        android:layout_marginTop="168dp"
        android:background="@color/with"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_navigation_bar_ac10">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="TextView"
            android:textColor="#121111"
            android:textSize="34sp"
            android:textStyle="bold" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>